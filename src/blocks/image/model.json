{
  "block_meta": {
    "BLOCK_REGISTER_NAME": "image",
    "BLOCK_TITLE": "Image",
    "keywords": ["image", "photo", "figure"],
    "hasSidebar": true,
    "hasExample": true,
    "deps": {
      "css": ["lazy-load-images", "shared-css-1", "shared-css-2"],
      "js": ["lazy-load-images", "shared-js-1", "shared-js-2"]
    }
  },
  "attributes": {
    "image": {
      "type": "object",
      "field_meta": {
        "type": "image",
        "label": "image"
      },
      "default": {
        "src": "",
        "alt": "",
        "subtype": "",
        "id": null
      }
    },
    "caption": {
      "type": "object",
      "field_meta": {
        "type": "text",
        "variation": "short",
        "label": "Caption (optional)",
        "max_chars": 200
      },
      "default": {
        "text": ""
      }
    },
    "link": {
      "type": "object",
      "field_meta": {
        "type": "cta",
        "label": "Link",
        "help": "Optional"
      },
      "default": {
        "url": "",
        "target": false
      }
    },

    "inspector_bg_is_dark": {
      "type": "object",
      "field_meta": {
        "type": "toggle",
        "label": "Dark background"
      },
      "default": {
        "checked": false
      }
    }
  }
}
